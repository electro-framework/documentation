<div class="row">

	<div id="sidebar" class="col-md-3">
		<div class="sidebar-nav">
		{% block sidebar %}
				{% include 'partials/sidebar.html.twig' %}
		{% endblock %}
		</div>
	</div>

	<div id="workspace" class="col-md-9">
		<div id="page" class="container-fluid">
			<h1>{{ page.title }}</h1>
			<div id="chapterContentsList" class="col-md-12 col-lg-12"></div>
			<div id="pageContent" class="content col-md-12 col-lg-12">
				{{ page.content }}
			</div>
		</div>
	</div>

</div>
